{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "jasmine": true
    },
    "plugins": ["jasmine"],
    "extends": ["angular", "eslint:recommended", "plugin:jasmine/recommended"],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":0,
        "angular/log": 0,
        "angular/controller-as": 0,
        "angular/module-setter": 0,
        "angular/module-getter": 0
    }
}