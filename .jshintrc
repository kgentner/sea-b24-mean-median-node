// Air BnB Style options for jshint
// from https://github.com/airbnb/javascript/blob/master/linters/jshintrc
{
  "browser": true,
  "jquery": true,
  "node": true,
  "camelcase": true,
  "eqeqeq": true,
  "indent": 2,
  "latedef": true,
  "maxlen": 80,
  "newcap": true,
  "quotmark": "single",
  "strict": true,
  "undef": true,
  "unused": true,
  "eqnull": true
}
